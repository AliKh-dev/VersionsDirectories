{
  "OpenTelemetry": {
    "Enable": true,
    "ServiceInstance": "01",
    "OtlpExporterUrl": "{OtelAddress}",
    "Tracing": {
      "Enable": true,
      "EnableOtlpExporter": true
    },
    "Metrics": {
      "Enable": true,
      "EnableOtlpExporter": true
    },
    "Logging": {
      "Enable": true,
      "EnableOtlpExporter": true,
      "SeverityLevel": "Debug"
    }
  }
}